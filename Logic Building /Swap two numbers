using a third variable 

#include <iostream>
using namespace std;

int main(){
    cout<< "Enter a";
    int a;
    cin >> a;
    
    cout<< "Enter b";
    int b;
    cin >> b;
    
    int temp = a;
    a=b;
    b=temp;
    
    cout << "a= " <<a;
    cout << "\n b= "<<b;
    
    return 0;
}


without using a third variable 

#include <iostream>
using namespace std;

int main(){
    cout<< "Enter a";
    int a;
    cin >> a;
    
    cout<< "Enter b";
    int b;
    cin >> b;
    
    a=a+b;
    b=a-b;
    a=a-b;
    
    cout << "a= " <<a;
    cout << "\nb= "<<b;
    
    return 0;
}
