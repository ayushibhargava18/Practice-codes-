#include <iostream>
using namespace std;

class pattern{
    public:
    
    void patternn(int x){
        for(int i=x; i>=1;i--){
            for (int j=i;j>=1;j--){
                cout<<j;
            }
        cout<<"\n";    
        }
        }
    };


int main(){
    pattern obj;
    obj.patternn(5);
    
}
