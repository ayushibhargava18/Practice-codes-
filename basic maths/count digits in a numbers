#include <iostream>
using namespace std;

class test{
    public:
    
    int exp(int x){
        int count=0;
        for(int i=x;i>0;i=i/10){
            count=count+1;
        }
    return count;
    }
};

int main(){
    cout<<"Enter a number";
    int x;
    cin>>x;
    
    test obj;
    cout<<obj.exp(x);
}
